@page
@model Web.Razor.Pages.LogoutModel

@{
    ViewData["Title"] = "Logged out";
}

<section class="mq-container mt-space fade-in">
    @await Html.PartialAsync("_PanelShell", new Web.Razor.Pages.Shared.PanelShellModel {
        Title = "Signed Out",
        Description = "You've been safely logged out.",
        Center = true,
        AdditionalClasses = "panel-p-xl panel-variant-success"
    })
        <img class="site_logo" src="/_content/Web.Razor/logo.png" alt="Site Logo" style="max-width:130px;" data-reveal />
        <div class="action-row mt-1_5" data-reveal>
            <a href="/en/signup" class="btn-modern btn-accent btn-pad-md">Sign In Again</a>
            <a href="/" class="btn-modern btn-pad-md">Return Home</a>
        </div>
    </div>
</section>